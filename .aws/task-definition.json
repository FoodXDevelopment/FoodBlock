{
    "containerDefinitions": [
        {
            "name": "server",
            "image": "560645079077.dkr.ecr.eu-west-2.amazonaws.com/foodblock-sandbox:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 3111,
                    "hostPort": 3111,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "PORT",
                    "value": "3111"
                },
                {
                    "name": "DATABASE_URL",
                    "value": "postgresql://foodblock:sandbox-dev-only@localhost:5432/foodblock"
                },
                {
                    "name": "BASE_PATH",
                    "value": "/foodblock"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/foodblock-sandbox",
                    "awslogs-region": "eu-west-2",
                    "awslogs-stream-prefix": "server"
                }
            }
        },
        {
            "name": "postgres",
            "image": "postgres:16-alpine",
            "cpu": 0,
            "portMappings": [],
            "essential": true,
            "environment": [
                {
                    "name": "POSTGRES_USER",
                    "value": "foodblock"
                },
                {
                    "name": "POSTGRES_PASSWORD",
                    "value": "sandbox-dev-only"
                },
                {
                    "name": "POSTGRES_DB",
                    "value": "foodblock"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/foodblock-sandbox",
                    "awslogs-region": "eu-west-2",
                    "awslogs-stream-prefix": "postgres"
                }
            }
        }
    ],
    "family": "foodblock-sandbox",
    "executionRoleArn": "arn:aws:iam::560645079077:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}
